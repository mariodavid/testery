<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="de.diedavids.testery.web.testaction.testactioncategory.TestactionCategoryEdit"
        datasource="testactionCategoryDs"
        focusComponent="fieldGroup"
        messagesPack="de.diedavids.testery.web.testaction.testactioncategory">
    <dsContext>
        <datasource id="testactionCategoryDs"
                    class="de.diedavids.testery.entity.testaction.TestactionCategory"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="testactionCategoryDs">
            <column width="250px">
                <field property="name"/>
                <field property="code"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
